<header>
    <div class="logo">
        <img src="/images/site/icon.png" alt="CSE Motors Logo">
        <h1>CSE Motors</h1>
        
    </div>

    <% if (locals.loggedin) { %>
        <div id="account_buttons">
            <button><a href="/account/">Welcome </a></button>
            <button><a href="/account/logout">Logout</a></button>
        </div>
    <% } else { %>
        <button><a href="/account/login" title="click to log in">My Account</a></button>
    <% } %>
  
</header>